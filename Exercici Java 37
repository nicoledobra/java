import java.util.Scanner;

public class Multiply {
        public static void main ( String[] args ) {
                Scanner sc = new Scanner ( System.in );
                int total = 1;
                int i = 2;
                while ( i < (2*10) ) {
                        System.out.println("Multiplying the number " + i);
                        total = total * i;
                        i = i + 2;
                }
                System.out.println ( "The total of the multiplication of the first ten even numbers is " + total );
        }
}
